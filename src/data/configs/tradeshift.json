{
    "curly": 2,
    "eqeqeq": 2,
    "guard-for-in": 2,
    "no-extend-native": 2,
    "no-caller": 2,
    "no-empty": 2,
    "no-new": 2,
    "no-undef": 2,
    "semi": 2,
    "dot-notation": 2,
    "quotes": [
      2,
      "single"
    ],
    "comma-style": [
      2,
      "last"
    ],

    "space-before-blocks": [
      1,
      "always"
    ],
    "no-bitwise": 1,
    "camelcase": 1,
    "wrap-iife": [
      1,
      "any"
    ],
    "no-use-before-define": [
      1,
      {
        "functions": false
      }
    ],
    "new-cap": 0,
    "no-plusplus": 1,
    "no-unused-vars": [
      1,
      {
        "args": "none"
      }
    ],
    "strict": 0,
    "no-cond-assign": 2,
    "no-debugger": 2,
    "no-eq-null": 2,
    "no-eval": 2,
    "no-unused-expressions": 2,
    "block-scoped-var": 1,
    "no-iterator": 2,
    "linebreak-style": 2,
    "no-loop-func": 2,
    "no-multi-str": 2,
    "no-proto": 2,
    "no-script-url": 1,
    "no-shadow": 1,
    "no-new-func": 2,
    "no-new-wrappers": 2,

    "no-invalid-this": 1,
    "brace-style": [
      1,
      "1tbs",
      {
        "allowSingleLine": true
      }
    ],
    "no-mixed-spaces-and-tabs": 1,
    "dot-location": [
      1,
      "property"
    ],
    "operator-linebreak": [
      1,
      "after"
    ],
    "padded-blocks": [
      1,
      "never"
    ],
    "quote-props": [
      1,
      "as-needed",
      {
        "keywords": true
      }
    ],
    "space-infix-ops": 1,
    "key-spacing": [
      1,
      {
        "beforeColon": false,
        "afterColon": true
      }
    ],
    "space-unary-ops": [
      1,
      {
        "words": false,
        "nonwords": false
      }
    ],
    "space-before-function-paren": [
      1,
      {
        "anonymous": "ignore",
        "named": "never"
      }
    ],
    "no-spaced-func": 1,
    "array-bracket-spacing": [
      1,
      "never"
    ],
    "no-trailing-spaces": 1,
    "yoda": [
      1,
      "never"
    ],
    "eol-last": 1,
    "one-var": [
      1,
      {
        "uninitialized": "always",
        "initialized": "never"
      }
    ],
    "keyword-spacing": [
      1,
      {
        "before": true,
        "after": true
      }
    ],
    "indent": [
      1,
      "tab",
      {
        "SwitchCase": 1
      }
    ]
  }
